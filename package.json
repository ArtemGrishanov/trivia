{
    "name": "trivia",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "@ckeditor/ckeditor5-build-balloon": "^16.0.0",
        "@ckeditor/ckeditor5-build-balloon-block": "^16.0.0",
        "@ckeditor/ckeditor5-build-classic": "^16.0.0",
        "@ckeditor/ckeditor5-build-inline": "^16.0.0",
        "@ckeditor/ckeditor5-editor-balloon": "^16.0.0",
        "@ckeditor/ckeditor5-react": "^2.0.0",
        "react": "^16.8.6",
        "react-dom": "^16.8.6",
        "react-quill": "^1.3.3",
        "react-redux": "^7.1.0",
        "react-scripts": "2.1.8",
        "react-sizeme": "^2.6.7",
        "redux": "^4.0.4",
        "redux-saga": "^1.1.1"
    },
    "scripts": {
        "build": "webpack --config webpack.prod.js",
        "css": "webpack --config webpack.css.js",
        "ui-demo": "webpack-dev-server --open --config webpack.ui-demo.js",
        "ui-demo-build": "webpack --config webpack.ui-demo.js",
        "babel": "babel --presets es2015 js/main.js -o build/main.bundle.js",
        "start": "webpack-dev-server --open --config webpack.dev.js",
        "eject": "react-scripts eject",
        "prettier:base": "./node_modules/.bin/prettier --write",
        "prettier": "npm run prettier:base -- \"{*.{js,json},src/**/*.{js,jsx,css,scss,less,json,yaml,yml,md}}\"",
        "precommit": "lint-staged"
    },
    "husky": {
        "hooks": {
            "pre-commit": "npm run precommit",
            "post-commit": "git update-index --again"
        }
    },
    "lint-staged": {
        "src/**/*.(js|jsx)": [
            "npm run prettier:base --",
            "git add"
        ],
        "**/*.{css,scss,md}": [
            "npm run prettier:base --",
            "git add"
        ]
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ],
    "devDependencies": {
        "@babel/core": "^7.5.4",
        "@babel/plugin-proposal-class-properties": "^7.5.0",
        "@babel/preset-env": "^7.5.4",
        "@babel/preset-react": "^7.0.0",
        "babel-loader": "^8.0.6",
        "css-loader": "^3.0.0",
        "html-loader": "^0.5.5",
        "html-webpack-plugin": "^3.2.0",
        "husky": "4.2.3",
        "lint-staged": "10.1.2",
        "mini-css-extract-plugin": "^0.8.0",
        "prettier": "2.0.4",
        "react-router-dom": "^5.1.2",
        "webpack": "^4.35.3",
        "webpack-cli": "^3.3.6",
        "webpack-dev-server": "^3.7.2",
        "webpack-merge": "^4.2.2"
    }
}
